package IO;

import java.io.File;

public class PrintFileName {

	public static void main(String[] args) {
		File src =new File("d:/");
		int index =0;
		
	}
	
	
	public static void printName(String src ,Integer i) {
		if(null== src ||!src.isEmpty()) {
			return;
		}
		
		else if(src.isDirectory()) {
			for(Integer t:i) {
				System.out.println("-");
			System.out.println(src);
			}
			
			printName(src, i++);
		
		}
		
	}
}
