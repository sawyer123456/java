package IO;

import java.io.File;

public class PrintFileName {

	public static void main(String[] args) {
		File src =new File("d:/");
		int index =0;
		printName(src, 0);
		
	}
	
	
	private static void printName(File src, int i) {
		
		if(null== src ||!src.isEmpty()) {
			return;
		}
		
		else if(src.()) {
			for(Integer t:i) {
				System.out.println("-");
			System.out.println(src);
			}
			
			printName(src, i++);
		
		}
	}


	
