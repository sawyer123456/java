public class smoke {
public static void main(String[] args) {
	new Thread().start();
}
}

class Father implements Runnable{
	@Override
	public void run() {
		System.out.println("想抽烟。。");
		System.out.println("没烟了，叫儿子买烟");
		
	}
	
}
