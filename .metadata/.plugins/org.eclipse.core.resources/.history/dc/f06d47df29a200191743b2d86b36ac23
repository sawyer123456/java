package IO;

import java.io.File;

public class PrintFileName {

	public static void main(String[] args) {
		File src = new File("d:/");
		int index = 0;
		printName(src, 0);

	}

	private static void printName(File src, int iss) {
		
		if(null== src ||!src.exists()) {
			return;
		}
		
		else if(src.isDirectory()) {
			for(File s:src.listFiles()) {
				System.out.println("-");
				
			}
			System.out.println(src);			
			printName(src, iss++);		
		}
	}
}